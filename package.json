{
  "name": "eeko",
  "version": "0.3.11",
  "type": "commonjs",
  "description": "Translation files manager",
  "license": "MIT",
  "author": "hovrcat",
  "private": false,
  "homepage": "https://github.com/IamRaduB/eeko#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/IamRaduB/eeko.git"
  },
  "bin": {
    "eeko": "./index.js"
  },
  "scripts": {
    "nx": "nx",
    "build": "",
    "bump-version:major": "npm version major && cd packages/core && npm version major",
    "bump-version:minor": "npm version minor && cd packages/core && npm version minor",
    "bump-version:patch": "npm version patch && cd packages/core && npm version patch",
    "build:lib-pack": "nx run core:build && nx run ui:build && bash ./tools/scripts/prepackage.sh &&  cd dist/core && npm pack",
    "commit-git": "git add . && git commit -m \"chore: add new release\"  && git push origin && git push origin --tags",
    "pre-publish:minor": "npm run bump-version:minor && npm run build:lib-pack && npm run commit-git",
    "pre-publish:major": "npm run bump-version:major && npm run build:lib-pack && npm run commit-git",
    "pre-publish:patch": "npm run bump-version:patch && npm run build:lib-pack && npm run commit-git"
  },
  "keywords": [
    "json",
    "i18n",
    "manager",
    "eeko",
    "cli",
    "ui"
  ],
  "devDependencies": {
